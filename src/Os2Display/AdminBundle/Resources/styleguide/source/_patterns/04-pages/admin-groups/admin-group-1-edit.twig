{% include "components-header" with { isActive: 5 } %}
{% include "modules-submenu" with { data: submenu } %}

<main role="main" class="content">
  <div class="content--inner">
    {% embed "modules-grid-halfs" %}
      {% block items %}
        {% embed "modules-grid-item" %}
          {% block content %}
            {% embed "modules-box" %}
              {% block header %}
                {% include "elements-heading-spacing-after" with { data: box.heading } %}
              {% endblock %}
              {% block content %}
                {% include "modules-forms-text-field" with { data: box.formName } %}
                {% include "elements-button-link-spacing-top" with { data: box.submit } %}
              {% endblock %}
            {% endembed %}
          {% endblock %}
        {% endembed %}
        {% embed "modules-grid-item" %}
          {% block content %}
            {% include "components-box-list" with { data: boxUsers } %}
          {% endblock %}
        {% endembed %}
      {% endblock %}
    {% endembed %}
  </div>
</main>

{% include "components-popup-add-group" with { data: popupAddToGroup } %}
